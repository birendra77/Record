<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_img"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginBottom="20dp"
            android:background="#2C3E50"
            android:orientation="horizontal">
            <androidx.cardview.widget.CardView
                android:id="@+id/club_back"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:backgroundTint="#2C3E50"
                android:layout_marginLeft="10dp"
                android:clickable="true"
                android:foreground="?selectableItemBackground"
                app:cardCornerRadius="20dp">
                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:src="@drawable/ic_baseline_arrow_back_24">

                </ImageView>
            </androidx.cardview.widget.CardView>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:layout_marginTop="20dp"
                android:paddingLeft="-50dp"
                android:layout_marginBottom="10dp"
                android:fontFamily="@font/aclonica"
                android:text="Contacts"
                android:textAlignment="center"
                android:textColor="#FDC689"
                android:textSize="35sp"
                android:textStyle="bold" />
        </LinearLayout>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginTop="10dp"
            android:backgroundTint="@color/start"
            android:clickable="true"
            android:contextClickable="true"
            android:foreground="?selectableItemBackground"
            app:cardCornerRadius="50dp"
            app:cardElevation="20dp">

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                app:cardCornerRadius="40dp"
                app:cardElevation="30dp">
                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:elevation="30dp"
                    android:src="@drawable/jd_logo">

                </ImageView>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/gdsc_ait"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:fontFamily="sans-serif-medium"
                android:gravity="center"
                android:paddingLeft="50dp"
                android:paddingTop="20dp"
                android:text="Joint Director\n8824990078"
                android:textColor="@color/teal_200"
                android:textSize="30sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/start"
            android:clickable="true"
            android:contextClickable="true"
            android:foreground="?selectableItemBackground"
            app:cardCornerRadius="50dp"
            app:cardElevation="20dp">


            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                app:cardCornerRadius="40dp"
                app:cardElevation="30dp">
                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:elevation="30dp"
                    android:background="@color/white"
                    android:src="@drawable/warden_img">

                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:id="@+id/oss_ait"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:paddingStart="30dp"
                android:fontFamily="sans-serif-medium"
                android:gravity="center"
                android:paddingTop="20dp"
                android:text="S B Shinde\n834867044"
                android:textColor="@color/teal_200"
                android:textSize="30sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/start"
            android:clickable="true"
            android:contextClickable="true"
            android:foreground="?selectableItemBackground"
            app:cardCornerRadius="50dp"
            app:cardElevation="20dp">


            <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            app:cardCornerRadius="40dp"
            app:cardElevation="30dp">
            <ImageView
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:elevation="30dp"
                android:background="@color/white"
                android:src="@drawable/warden_img">

            </ImageView>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/tb_ait"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:fontFamily="sans-serif-medium"
                android:paddingStart="60dp"
                android:gravity="center"
                android:paddingTop="20dp"
                android:text="Sandeep Mahadik\n8087991775"
                android:textColor="@color/teal_200"
                android:textSize="30sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/start"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            app:cardCornerRadius="50dp"
            app:cardElevation="20dp">


            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                app:cardCornerRadius="40dp"
                app:cardElevation="30dp">
            <ImageView
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:elevation="30dp"
                android:background="@color/white"
                android:src="@drawable/warden_img">

            </ImageView>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/sports_ait"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:fontFamily="sans-serif-medium"
                android:gravity="center"
                android:paddingTop="25dp"
                android:text="Warden 3"
                android:textColor="@color/teal_200"
                android:textSize="30sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/start"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            app:cardCornerRadius="50dp"
            app:cardElevation="20dp">

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                app:cardCornerRadius="40dp"
                app:cardElevation="30dp">
            <ImageView
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:elevation="30dp"
                android:background="@color/white"
                android:src="@drawable/warden_img">

            </ImageView>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/ecell_ait"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:fontFamily="sans-serif-medium"
                android:gravity="center"
                android:paddingTop="25dp"
                android:text="Warden 4"
                android:textColor="@color/teal_200"
                android:textSize="30sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/start"
            android:clickable="true"
            android:foreground="?selectableItemBackground"
            app:cardCornerRadius="50dp"
            app:cardElevation="20dp">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:fontFamily="@font/amarante"
                android:gravity="center"
                android:text="Contact them in Emergency!"
                android:textColor="@color/teal_200"
                android:textSize="30sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>


    </LinearLayout>
</ScrollView>